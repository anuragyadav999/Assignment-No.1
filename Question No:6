//6. WAP to print sum of two numbers. Numbers are taken by user.

#include <stdio.h>

int main() {
    int a,b, sum;

    printf("Enter the first number here ");
    scanf("%d", &a);

    printf("Enter the second number here ");
    scanf("%d", &b);
    sum = a + b;

    printf("The sum of the two numbers %d and %d is equal to %d",a,b,sum);
}
